<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.StoreEntity" table="store" schema="hibernate">
        <id name="id" column="id"/>
        <property name="storeName" column="storeName"/>
        <property name="item" column="item"/>
        <property name="number" column="number"/>
        <property name="itemId" column="itemId"/>
        <property name="storeManager" column="storeManager"/>
    </class>
</hibernate-mapping>